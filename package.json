    "fs-extra": "^11.1.1",
    "get-port": "^7.0.0",
    "lightningcss": "^1.21.5",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "mini-css-extract-plugin": "^2.7.6",
    "moment-timezone-data-webpack-plugin": "^1.5.1",
    "nodemon": "^3.0.1",
    "p-map": "^4.0.0",
    "p-queue": "^6.6.2",
    "pino-pretty": "^10.2.0",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.3.3",
    "postcss-preset-env": "^9.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-refresh": "^0.14.0",
    "resolve-url-loader": "^5.0.0",
    "rimraf": "^5.0.1",
    "sass": "^1.69.0",
    "sass-loader": "^13.3.2",
